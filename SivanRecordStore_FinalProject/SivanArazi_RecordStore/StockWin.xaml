<Window x:Class="SivanArazi_RecordStore.StockWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SivanArazi_RecordStore"
        mc:Ignorable="d"
        Title="StockWin" Height="450" Width="800" Background="#DA7808">
    <Grid>
        <DataGrid x:Name="dg_Stock" Margin="0,22,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <Button Content="Back to Menu" HorizontalAlignment="Right" VerticalAlignment="Top" Width="90" Height="22" Click="BackToMenu"/>
    </Grid>
</Window>
